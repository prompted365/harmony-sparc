{
  "name": "@agentflow/api",
  "version": "0.1.0",
  "description": "High-performance Express.js API for AgentFlow platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='tests/.*\\.test\\.ts$'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:integration:full": "ts-node tests/run-integration-tests.ts",
    "test:e2e": "jest --testPathPattern='end-to-end'",
    "test:performance": "jest --testNamePattern='Performance|performance'",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "performance:monitor": "ts-node scripts/performance-monitor.ts",
    "performance:validate": "ts-node scripts/run-performance-validation.ts",
    "performance:load": "artillery run load-test.yml",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "health-check": "curl -f http://localhost:3000/health || exit 1"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "express-validator": "^7.0.1",
    "ethers": "^6.8.1",
    "zod": "^3.22.4",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2",
    "artillery": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}