{
  "name": "harmony-sparc",
  "version": "1.0.0",
  "description": "Harmony SPARC - Advanced blockchain protocol with ML and quantum computing",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm run build && nodemon",
    "build": "tsc && tsc-alias",
    "build:perf": "NODE_ENV=production npm run build",
    "start": "node dist/index.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:unit:api": "jest --testPathPattern=test/unit/api",
    "test:unit:contracts": "jest --testPathPattern=test/unit/contracts",
    "test:unit:ml": "jest --testPathPattern=test/unit/ml",
    "test:unit:quantum": "jest --testPathPattern=test/unit/quantum",
    "test:integration": "jest --testPathPattern=test/integration --runInBand",
    "test:e2e": "jest --testPathPattern=test/e2e --runInBand",
    "test:performance": "jest --testPathPattern=test/performance --runInBand",
    "test:load": "npm run build && node test/load/runner.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contracts": "hardhat test",
    "test:contracts:coverage": "hardhat coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"agentflow/**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "DATABASE_URL=$TEST_DATABASE_URL prisma migrate dev",
    "db:generate": "prisma generate",
    "analyze:load-test": "node scripts/analyze-load-test.js",
    "wait-for-services": "node scripts/wait-for-services.js",
    "hardhat:node": "hardhat node",
    "hardhat:compile": "hardhat compile",
    "hardhat:deploy": "hardhat run scripts/deploy.ts",
    "hardhat:verify": "hardhat verify"
  },
  "keywords": [
    "blockchain",
    "cryptocurrency",
    "defi",
    "machine-learning",
    "quantum-computing",
    "sparc",
    "harmony"
  ],
  "author": "Harmony SPARC Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@tensorflow/tfjs-node": "^4.16.0",
    "@types/node": "^20.11.5",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.14.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "node-fetch": "^2.7.0",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@openzeppelin/contracts": "^5.0.1",
    "@openzeppelin/hardhat-upgrades": "^3.0.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.4.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "chai": "^4.3.10",
    "claude-flow": "^1.0.72",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "hardhat": "^2.19.4",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mocha-multi-reporters": "^1.5.1",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.2",
    "prisma": "^5.8.0",
    "solidity-coverage": "^0.8.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
